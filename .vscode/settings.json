{
    "prettier.bracketSpacing": false,
    "nuxt.isNuxtApp": false,
    "files.associations": {
        "*.yaml": "home-assistant"
    },
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize",
    "editor.detectIndentation": false,
    "editor.codeActionsOnSave": {
    },
    "[markdown]": {
      "editor.quickSuggestions": {
        "other": true, // this must be true
        "comments": false,
        "strings": false
    }
  }
}